{
    "name": "whatsapp-web.js",
    "version": "1.34.1",
    "description": "Library for interacting with the WhatsApp Web API ",
    "main": "./index.js",
    "typings": "./index.d.ts",
    "scripts": {
        "test": "mocha tests --recursive --timeout 5000",
        "test-single": "mocha",
        "shell": "node --experimental-repl-await ./shell.js",
        "generate-docs": "npx jsdoc --configure .jsdoc.json --verbose",
        "start": "node app.js",
        "start:https": "node app.js",
        "stop": "node service.js stop",
        "restart": "node service.js restart",
        "status": "node service.js status",
        "logs": "node service.js logs",
        "start:dev": "node app.js",
        "start:service": "node service.js start",
        "setup": "node -e \"console.log('Setup complete! Run npm start to begin')\"",
        "ssl:generate": "node generate-ssl.js",
        "ssl:check": "node -e \"const SSLGenerator = require('./generate-ssl'); const ssl = new SSLGenerator(); console.log(ssl.getCertificates() ? 'SSL certificates found' : 'No SSL certificates');\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pedroslopez/whatsapp-web.js.git"
    },
    "keywords": [
        "whatsapp",
        "whatsapp-web",
        "api",
        "bot",
        "client",
        "node"
    ],
    "author": "Pedro Lopez",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/pedroslopez/whatsapp-web.js/issues"
    },
    "homepage": "https://wwebjs.dev/",
    "dependencies": {
        "@google/generative-ai": "^0.24.1",
        "@pedroslopez/moduleraid": "^5.0.2",
        "compression": "^1.8.1",
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "fluent-ffmpeg": "2.1.3",
        "helmet": "^8.1.0",
        "mime": "^3.0.0",
        "node-fetch": "^2.6.9",
        "node-webpmux": "3.1.7",
        "puppeteer": "^18.2.1",
        "qrcode": "^1.5.4",
        "socket.io": "^4.8.1"
    },
    "devDependencies": {
        "@types/node-fetch": "^2.5.12",
        "chai": "^4.3.4",
        "chai-as-promised": "^7.1.1",
        "dotenv": "^16.6.1",
        "eslint": "^8.4.1",
        "eslint-plugin-mocha": "^10.0.3",
        "jsdoc": "^3.6.4",
        "jsdoc-baseline": "^0.1.5",
        "mocha": "^9.0.2",
        "sinon": "^13.0.1"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "optionalDependencies": {
        "archiver": "^5.3.1",
        "fs-extra": "^10.1.0",
        "unzipper": "^0.10.11"
    }
}
